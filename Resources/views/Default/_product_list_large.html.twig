<div class="v-product-list-large">
    <table>

    {% for product in productsPagination %}
        <tr>
            <td class="v-product-name">
                <a href="{{ path('v_product_detail', { 'slug': product.slug }) }}">
                {{ product.name }}</a>
            </td class="v-product-price">
            <td> 0 {# {{ product.pricing['unit'] | price_format(store.defaultCurrency) }} #}</td>
        </tr>

    {% else %}
    No products loaded.
    {% endfor %}

    </table>
    <div class="row">
        <div class="navigation">
            {{ productsPagination.render()|raw }}
        </div>
    </div>
</div>
