           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="vespolina_commerce.fulfillment_method_resolver.class">Vespolina\CommerceBundle\Resolver\SimpleFulfillmentMethodResolver</parameter>
        <parameter key="vespolina_commerce.fulfillment_method_resolver.configuration">Dummy</parameter>
