{
    "name": "vespolina/commerce-bundle",
    "type": "symfony-bundle",
    "description": "Vespolina Commerce bundle. Integrates vespolina libraries with Symfony2",
    "keywords": ["vespolina", "shop", "ecommerce", "store", "item"],
    "homepage": "http://vespolina-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina Team",
            "homepage": "https://github.com/vespolina/VespolinaCommerceBundle/contributors"
        }
    ],
    "require": {
        "friendsofsymfony/jsrouting-bundle": "*",
        "friendsofsymfony/rest-bundle": "*",
        "php": ">=5.4.0",
        "sp/bower-bundle": "*",
        "symfony/symfony": ">=2.1",
        "omnipay/omnipay": "~2.0",
        "vespolina/action": "*",
        "vespolina/commerce": "dev-master",
        "vespolina/taxonomy": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpspec/phpspec": "dev-master"
    },
    "suggest": {
        "friendsofsymfony/user-bundle": "Create a application user based on a partners"
    },
    "autoload": {
        "psr-0": { "Vespolina\\CommerceBundle": "" }
    },
    "target-dir": "Vespolina/CommerceBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}

